#include<bits/stdc++.h>
//#include<queue>
using namespace std;
int main()
{
	//定义一个queue ，不能在初始化的时候赋值
	queue<int>sample; 
	
	//使用push在末尾放入一个元素
	sample.push(1); 
	sample.push(2); 
	sample.push(3); 
	sample.push(4);
	
	//删除第一个元素
	sample.pop();
	//判断queue是否为空
	cout << sample.empty() << endl; 
	//查看队列第一个元素
	cout << sample.front() << endl; 
	//查看队列最后一个元素
	cout << sample.back() << endl; 
	//查看队列的大小
	cout << sample.size() << endl; 
	
	
	//queue没有迭代器
	//队列本身就是一种先进先出的限制性数据结构
	//因此只能通过 front（）来访问队首元素，或是通过back（）来访问队尾元素。
}
